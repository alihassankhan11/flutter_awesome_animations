+import 'dart:math';
+import 'dart:ui';
+
+import 'package:flame/flame.dart';
+import 'package:flame/game.dart';
+import 'package:flame/gestures.dart';
+import 'package:flutter/gestures.dart';
+import 'package:my_porto/components/main_char.dart';
+
+class BoxGame extends Game with TapDetector {
+  Size screenSize;
+  bool hasWon = false;
+  double tileSize;
+  List<MainChar> mainChar = [];
+  Random rnd = Random();
+
+  BoxGame() {
+    initialize();
+  }
+
+  void initialize() async {
+    resize(await Flame.util.initialDimensions());
+
+    spawnFly();
+  }
+
+  void render(Canvas canvas) {
+    //black background
+    Rect bgRect = Rect.fromLTWH(0, 0, screenSize.width, screenSize.height);
+    Paint bgPaint = Paint();
+    bgPaint.color = Color(0xff000000);
+    canvas.drawRect(bgRect, bgPaint);
+
+    // draw a white box
+    mainChar.forEach((MainChar fly) => fly.render(canvas));
+  }
+
+  void update(double t) {
+    mainChar.forEach((MainChar fly) => fly.update(t));
+  }
+
+  void resize(Size size) {
+    screenSize = size;
+    tileSize = screenSize.width / 9;
+    super.resize(size);
+  }
+
+  void onTapDown(TapDownDetails d) {
+    print("Player tap down on ${d.globalPosition.dx} - ${d.globalPosition.dy}");
+    double screenCenterX = screenSize.width / 2;
+    double screenCenterY = screenSize.height / 2;
+    if (d.globalPosition.dx >= screenCenterX - 75 &&
+        d.globalPosition.dx <= screenCenterX + 75 &&
+        d.globalPosition.dy >= screenCenterY - 75 &&
+        d.globalPosition.dy <= screenCenterY + 75) {
+      hasWon = true;
+    }
+  }
+
+  @override
+  void onTapUp(TapUpDetails details) {
+    print(
+        "Player tap up on ${details.globalPosition.dx} - ${details.globalPosition.dy}");
+  }
+
+  void spawnFly() {
+    double x = rnd.nextDouble() * (screenSize.width - tileSize);
+    double y = rnd.nextDouble() * (screenSize.height - tileSize);
+
+    mainChar.add(MainChar(this, x, y));
+  }
+}
