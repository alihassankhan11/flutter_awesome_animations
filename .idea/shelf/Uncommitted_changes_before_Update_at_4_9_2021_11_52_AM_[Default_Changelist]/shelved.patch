+import 'package:flutter/material.dart';
+import 'package:flutter_svg/svg.dart';
+
+import '../res.dart';
+
+class SpinKitPulse extends StatefulWidget {
+  const SpinKitPulse({
+    Key key,
+    this.color,
+    this.size = 2000.0,
+    this.itemBuilder,
+    this.duration = const Duration(seconds: 1),
+    this.controller,
+  })  : assert(
+            !(itemBuilder is IndexedWidgetBuilder && color is Color) &&
+                !(itemBuilder == null && color == null),
+            'You should specify either a itemBuilder or a color'),
+        super(key: key);
+
+  final Color color;
+  final double size;
+  final IndexedWidgetBuilder itemBuilder;
+  final Duration duration;
+  final AnimationController controller;
+
+  @override
+  _SpinKitPulseState createState() => _SpinKitPulseState();
+}
+
+class _SpinKitPulseState extends State<SpinKitPulse>
+    with TickerProviderStateMixin {
+  AnimationController _controller;
+  AnimationController _controller_1;
+  Animation<double> _animation;
+  Animation<double> positionAnimation;
+  bool animationStatus = true;
+  bool loaded = false;
+
+  @override
+  void initState() {
+    super.initState();
+
+    _controller = (widget.controller ??
+        AnimationController(vsync: this, duration: widget.duration))
+      ..addListener(() => setState(() {}));
+    _animation = CurveTween(curve: Curves.easeInOut).animate(_controller);
+
+    _controller_1 = AnimationController(
+      duration: const Duration(seconds: 2),
+      vsync: this,
+    )
+      ..repeat()
+      ..addStatusListener((status) {
+        if (status == AnimationStatus.completed) {
+          setState(() => animationStatus = false);
+          _controller.repeat();
+        }
+      });
+
+    positionAnimation = Tween<double>(
+      begin: 0.1,
+      end: 0.5,
+    ).animate(_controller_1);
+  }
+
+  @override
+  void dispose() {
+    _controller.dispose();
+    super.dispose();
+  }
+
+  @override
+  Widget build(BuildContext context) {
+    return Transform.scale(
+      scale: positionAnimation.value,
+      child: Opacity(
+        opacity: 1.0 - _animation.value,
+        child: Transform.scale(
+          scale: animationStatus ? 0.1 : _animation.value,
+          child: SizedBox.fromSize(
+            size: Size.square(widget.size),
+            child: _itemBuilder(0),
+          ),
+        ),
+      ),
+    );
+  }
+
+  Widget _itemBuilder(index) => widget.itemBuilder != null
+      ? widget.itemBuilder(context, index)
+      : SvgPicture.asset(Res.user);
+}
